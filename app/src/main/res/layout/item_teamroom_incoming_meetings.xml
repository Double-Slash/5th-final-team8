<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="170dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tr_im_subject"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        android:text="더블슬래시 8"
        app:layout_constraintLeft_toLeftOf="parent"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tr_im_when"
        android:text="월요일 16:00 ~ 20:00"
        app:layout_constraintTop_toBottomOf="@id/tr_im_subject"
        app:layout_constraintRight_toRightOf="parent"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tr_im_where"
        android:text="8"
        app:layout_constraintTop_toBottomOf="@id/tr_im_when"
        app:layout_constraintRight_toRightOf="parent"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tr_rm_alart"
        app:layout_constraintTop_toBottomOf="@id/tr_im_when"
        app:layout_constraintRight_toRightOf="parent">

    </TextView>





</androidx.constraintlayout.widget.ConstraintLayout>