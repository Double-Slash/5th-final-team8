<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="170dp"
    android:layout_height="170dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tr_rm_subject"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        android:text="팀플명"
        app:layout_constraintLeft_toLeftOf="parent"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tr_rm_peoples"
        android:text="참여인원"
        app:layout_constraintTop_toBottomOf="@id/tr_rm_subject"
        app:layout_constraintLeft_toLeftOf="parent"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tr_rm_progress"
        android:text="진행중"
        app:layout_constraintTop_toBottomOf="@id/tr_rm_peoples"
        app:layout_constraintLeft_toLeftOf="parent"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tr_rm_isConfirmed"
        app:layout_constraintTop_toBottomOf="@id/tr_rm_progress"
        app:layout_constraintLeft_toLeftOf="parent">

    </TextView>





</androidx.constraintlayout.widget.ConstraintLayout>